<%- include('parts/header') %>

<!-- HEADER -->
<header class="site-header">
    <div class="flex-container">
        <a class="site-logo" href="/">
            <img src="/img/logo.svg" alt="Logo Az-Pharm">
        </a>
        <nav class="site-nav site-nav-close">
            <ul class="site-nav__ul">
                <li class="site-nav__li">
                    <a class="site-nav__link" href="#how-we-work">Biz qanday ishlaymiz?</a>
                </li>
                <li class="site-nav__li">
                    <a class="site-nav__link" href="#about-us">Biz haqimizda</a>
                </li>
                <li class="site-nav__li">
                    <a class="site-nav__link" href="#gallery">Galereya</a>
                </li>
                <li class="site-nav__li">
                    <a class="site-nav__link" href="#feedback">Mijozlar fikri</a>
                </li>
                <li class="site-nav__li">
                    <a class="site-nav__link" href="#contact">Aloqa</a>
                </li>
            </ul>
        </nav>
        <div class="phone">
            <a class="phone__link" href="tel: <%= data.home.phoneNumber %>">
                <svg width="18" height="18" viewBox="0 0 18 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M6.366 7.682C7.30434 9.33048 8.66952 10.6957 10.318 11.634L11.202 10.396C11.3442 10.1969 11.5543 10.0569 11.7928 10.0023C12.0313 9.94779 12.2814 9.98254 12.496 10.1C13.9103 10.8729 15.4722 11.3378 17.079 11.464C17.3298 11.4839 17.5638 11.5975 17.7345 11.7823C17.9052 11.9671 18 12.2094 18 12.461V16.923C18.0001 17.1706 17.9083 17.4094 17.7424 17.5932C17.5765 17.777 17.3483 17.8927 17.102 17.918C16.572 17.973 16.038 18 15.5 18C6.94 18 0 11.06 0 2.5C0 1.962 0.027 1.428 0.082 0.898C0.107255 0.651697 0.222984 0.423521 0.40679 0.257634C0.590595 0.0917472 0.829406 -5.33578e-05 1.077 2.32673e-08H5.539C5.79056 -3.15185e-05 6.0329 0.0947515 6.21768 0.265451C6.40247 0.43615 6.51613 0.670224 6.536 0.921C6.66222 2.52779 7.12708 4.08968 7.9 5.504C8.01746 5.71856 8.05221 5.96874 7.99767 6.2072C7.94312 6.44565 7.80306 6.65584 7.604 6.798L6.366 7.682ZM3.844 7.025L5.744 5.668C5.20478 4.50409 4.83535 3.26884 4.647 2H2.01C2.004 2.166 2.001 2.333 2.001 2.5C2 9.956 8.044 16 15.5 16C15.667 16 15.834 15.997 16 15.99V13.353C14.7312 13.1646 13.4959 12.7952 12.332 12.256L10.975 14.156C10.4287 13.9437 9.89801 13.6931 9.387 13.406L9.329 13.373C7.36758 12.2567 5.74328 10.6324 4.627 8.671L4.594 8.613C4.30691 8.10199 4.05628 7.57134 3.844 7.025Z"
                          fill="#0550C1"/>
                </svg>
                <span><%= data.home.phoneNumber %></span>
            </a>
        </div>

        <svg class="hamburger" width="27" height="18" viewBox="0 0 27 18" fill="none" xmlns="http://www.w3.org/2000/svg">
            <line y1="1" x2="27" y2="1" stroke="#0550C1" stroke-width="2"/>
            <line x1="8" y1="9" x2="27" y2="9" stroke="#0550C1" stroke-width="2"/>
            <line y1="17" x2="27" y2="17" stroke="#0550C1" stroke-width="2"/>
        </svg>
    </div>
</header>

<!--INTRO-->
<section class="section intro">
    <div class="flex-container">
        <div class="intro__left">
            <div class="intro__company">Az-Pharm</div>
            <h1 class="intro__heading">Online dorixona</h1>
            <p class="intro__text">Buyurtma berish uchun ism, telefon raqam va dori nomini kiriting</p>
            <form class="form enroll-form">
                <label class="form__label">
                    <span>Ism</span>
                    <input class="input ism" type="text" placeholder="Ism..." required>
                </label>
                <label class="form__label">
                    <span>Telefon raqami</span>
                    <input class="input telefon" id="phone" type="tel" value="+998" placeholder="Telefon" required>
                </label>
                <label class="form__label">
                    <span>Dori nomi(qisqa)</span>
                    <input class="input dori-nomi" type="text" placeholder="(Parasetomol va dimitrol)" required>
                </label>
                <button class="button">Yuborish</button>
                <div class="form__sent">
                    <div class="success-sender">

                    </div>
                </div>
            </form>
        </div>
        <div class="intro__right">
            <img src="/img/intro-img.png" alt="Intro illustrator">
        </div>
        <div class="intro__decoration">
            <img src="/img/bg/bg-img.png" alt="">
        </div>
    </div>
</section>

<!--SLIDER-->
<section class="section slider">
    <div class="container">
        <div class="splide single-slider">
            <div class="splide__track">
                <ul class="splide__list">
                    <% if(data.slides && data.slides.length>0) { %>
                        <% for(slide of data.slides) { %>
                            <li class="splide__slide single-slider__slide">
                                <img src="<%= slide.image %> " alt="">
                                <div class="single-slider__layer">
                                    <div class="single-slider__content">
                                        <h3 class="single-slider__heading"><%= slide.title %> </h3>
                                        <p class="single-slider__text"><%= slide.description %> </p>
                                        <a class="single-slider__button button" href="<%= data.home.telegram %>">Buyurtma berish</a>
                                    </div>
                                </div>
                            </li>
                        <% } %>     
                    <% } else { %>
                        <li class="splide__slide single-slider__slide">
                            <img src="https://picsum.photos/1200/370?rand=1" alt="">
                            <div class="single-slider__layer">
                                <div class="single-slider__content">
                                    <h3 class="single-slider__heading">Slide not found!!!</h3>
                                </div>
                            </div>
                        </li>
                     <% } %>    
                </ul>
            </div>
        </div>
    </div>
</section>

<!--HOW WE WORK?-->
