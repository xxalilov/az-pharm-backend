<!--ABOUT US-->
<section class="section about-us" id="about-us">
    <div class="flex-container">
        <div class="about-us__video video">
            <div class="video__layer">
                <button class="video__play" data-video="<%= data.about[0]?.video %>">
                    <svg width="27" height="37" viewBox="0 0 27 37" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M26.5 18.5L0.5 0V37L26.5 18.5Z" fill="white"/>
                    </svg>
                </button>
            </div>
        </div>
        <div class="about-us__info">
            <h2 class="section__heading about-us__heading"><%= data.about[0]?.title %></h2>
            <p class="about-us__text"><%= data.about[0]?.description %></p>
            <a class="button" href="<%= data.home.telegram %>">Konsultatsiya olish</a>
        </div>
    </div>
    </div>
</section>

<!--GALLERY-->
<section class="section gallery" id="gallery">
    <div class="container">
        <h2 class="section__heading">Galereya</h2>
    </div>
    <div class="grid-container">
        <% if(data.gallery && data.gallery.length === 5) { %>
            <div class="gallery__left">
                <div class="gallery__item">
                    <img src="<%= data.gallery[0].image %>" alt="">
                </div>
                <div class="gallery__item">
                    <img src="<%= data.gallery[1].image %>" alt="">
                </div>
                <div class="gallery__item">
                    <img src="<%= data.gallery[2].image %>" alt="">
                </div>
                <div class="gallery__item">
                    <img src="<%= data.gallery[3].image %>" alt="">
                </div>
            </div>
            <div class="gallery__right">
                <img src="<%= data.gallery[4].image %>" alt="">
            </div>
         <% } else { %>
            <div class="gallery__left">
                <div class="gallery__item">
                    <img src="https://picsum.photos/285?rand=1" alt="">
                </div>
                <div class="gallery__item">
                    <img src="https://picsum.photos/285?rand=2" alt="">
                </div>
                <div class="gallery__item">
                    <img src="https://picsum.photos/285?rand=3" alt="">
                </div>
                <div class="gallery__item">
                    <img src="https://picsum.photos/285?rand=4" alt="">
                </div>
            </div>
            <div class="gallery__right">
                <img src="https://picsum.photos/590?rand=5" alt="">
            </div>
         <% } %>    
    </div>
</section>

<!--CONTACT-->
<section class="section contact" id="contact">
    <div class="flex-container">
        <div class="contact__left">
            <img src="/img/contact/contact.png" alt="">
        </div>
        <div class="contact__right">
            <h2 class="section__heading">Aloqa</h2>
            <p class="contact__text">Shunchaki ism va telefon raqamingizni qoldiring. Biz siz bilan darhol aloqaga chiqamiz</p>
            <form class="form contact-form">
                <label class="form__label">
                    <span>Ism</span>
                    <input class="input contact-name" type="text" placeholder="Ism..." required>
                </label>
                <label class="form__label">
                    <span>Telefon raqami</span>
                    <input class="input contact-phone" id="phone-2" type="tel" value="+998" placeholder="Telefon" required>
                </label>
                <label class="form__label">
                    <span>Murojaat matni (ixtiyoriy)</span>
                    <textarea class="input input--textarea contact-message" placeholder="Murojaat matni..."></textarea>
                </label>
                <button class="button">Yuborish</button>
                <div class="form__sent sec-send">
                 
                </div>
            </form>
        </div>
        <div class="contact__decoration">
            <img src="/img/bg/bg-img.png" alt="">
        </div>
    </div>
</section>

<!--FEEDBACK-->
<section class="section feedback" id="feedback">
    <div class="container">
        <h2 class="section__heading">Mijozlar fikri</h2>
        <div class="splide feedback-slider">
            <div class="splide__track">
                <ul class="splide__list">
                    <% if(data.feedbacks && data.feedbacks.length > 0) {%>
                        <% for(let feedback of data.feedbacks){ %>
                            <li class="splide__slide">
                                <div class="feedback__item">
                                    <div class="feedback__content">
                                        <h3 class="feedback__title"><%= feedback.title %></h3>
                                        <p class="feedback__text"><%= feedback.description %></p>
                                    </div>
                                    <div class="feedback__author author">
                                        <div class="author__img">
                                            <img src="<%= feedback.image %>" alt="">
                                        </div>
                                        <div class="author__info">
                                            <div class="author__name"><%= feedback.authorName %></div>
                                            <div class="author__job"><%= feedback.authorProfession %></div>
                                        </div>
                                    </div>
                                </div>
                            </li>
                         <% } %>    
                     <% } else { %>
                        <li class="splide__slide">
                            <div class="feedback__item">
                                <div class="feedback__content">
                                    <h3 class="feedback__title">Shunchaki professional xizmat</h3>
                                    <p class="feedback__text">Assalomu alaykum, Dorixona haqida izoh qoldirmoqchi edim, o’sha kuni juda ham kech
                                        edi, birdan qizimni istimasi chiqishni boshladi, bizni atrofimizda dorixonalar yo’q,
                                        taksi chaqirib dorixona oxtarishni boshladim</p>
                                </div>
                                <div class="feedback__author author">
                                    <div class="author__img">
                                        <img src="https://i.pravatar.cc/70?img=44" alt="">
                                    </div>
                                    <div class="author__info">
                                        <div class="author__name">Nargiza Abdullayeva</div>
                                        <div class="author__job">Student</div>
                                    </div>
                                </div>
                            </div>
                        </li>
                     <% } %>    
                </ul>
            </div>
        </div>
    </div>
</section>

<!--FOOTER-->
<footer class="site-footer">
    <div class="flex-container">
        <a class="site-logo" href="/">
            <img src="/img/logo.svg" alt="Logo Az-Pharm">
        </a>

        <p class="footer__text">AZ-PHARM barcha huquqlar ximoyalangan!</p>
        
        <ul class="footer__socials socials">
            <li class="socials__item">
