<%- include('../includes/head') %>

<div class="wrapper">
  <%- include('../includes/sidebar') %>
  <div class="main-panel">
    <%- include('../includes/navbar') %>
    <div class="content">
      <div class="row">
        <% if(enrolls.length > 0) { %> <% for(let enroll of enrolls) { %>
        <div class="col-lg-4">
          <div class="card card-chart">
            <div class="card-header">
              <div class="d-flex justify-content-between">
                <span>
                  <button class="btn delete-enroll" value="<%= enroll._id %>">
                    <i class="tim-icons icon-trash-simple text-danger"></i>
                  </button>
                </span>
              </div>
            </div>
            <div class="card-body mx-auto">
              <h4>name: <%= enroll.name %></h4>
              <h4>phone: <%= enroll.phone %></h4>
              <h4>pill: <%= enroll.pillName %></h4>
            </div>
          </div>
        </div>
        <% } %> <% }else { %>
        <h1 class="mx-auto">Enroll Not Found!</h1>
        <% } %>
      </div>
    </div>
  </div>
</div>
<%- include('../includes/fixed_plugin') %> <%- include('../includes/end') %>
